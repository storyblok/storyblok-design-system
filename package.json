{
  "name": "@storyblok/design-system",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/storyblok-design-system.git"
  },
  "scripts": {
    "dev": "yarn storybook",
    "build": "vite build",
    "test:unit": "jest test:unit",
    "lint": "eslint src src/ tests/ .storybook/",
    "lint:style": "stylelint 'src/**/*.{vue,scss}' --config ./.stylelintrc.json --fix",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006 --ci",
    "prepare": "is-ci || husky install",
    "pre-commit": "lint-staged"
  },
  "main": "dist/storyblok-design-system.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "click-outside-vue3": "^4.0.1",
    "dayjs": "^1.11.7",
    "maska": "^1.5.0",
    "throttle-debounce": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@storybook/addon-a11y": "^7.0.0-rc.8",
    "@storybook/addon-essentials": "^7.0.0-rc.8",
    "@storybook/addon-storysource": "^7.0.0-rc.8",
    "@storybook/addon-viewport": "^7.0.0-rc.8",
    "@storybook/addons": "^7.0.0-rc.8",
    "@storybook/theming": "^7.0.0-rc.8",
    "@storybook/vue3": "^7.0.0-rc.8",
    "@storybook/vue3-vite": "^7.0.0-rc.8",
    "@testing-library/vue": "^7.0.0",
    "@vitejs/plugin-vue": "^4",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.3.2",
    "@vue/vue3-jest": "^29.2.3",
    "babel-jest": "^29.5.0",
    "babel-preset-vue": "^2.0.2",
    "chromatic": "^6.17.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.60.0",
    "sass-mq": "^6.0.0",
    "storybook": "^7.0.0-rc.8",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "~5.0.2",
    "vite": "^4",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.2.47",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore"
    ],
    "*.{vue,scss,css}": [
      "stylelint --config .stylelintrc.json --fix"
    ]
  },
  "release": {
    "branches": [
      "next"
    ]
  }
}
