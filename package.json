{
  "name": "storyblok-design-system",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib ./src/main.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint src/ tests/ .storybook/",
    "build-storybook": "build-storybook -s ./public",
    "storybook": "start-storybook -p 6006 -s ./public --ci",
    "prepare": "npm run build"
  },
  "main": "dist/storyblok-design-system.common.js",
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "dayjs": "^1.10.7",
    "portal-vue": "^2.1.7",
    "throttle-debounce": "^2.3.0",
    "vue-fragment": "^1.5.2"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addons": "^6.3.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/theming": "^6.3.12",
    "@storybook/vue": "^6.3.12",
    "@vue/cli-plugin-babel": "~4.5.14",
    "@vue/cli-plugin-eslint": "~4.5.14",
    "@vue/cli-plugin-unit-jest": "~4.5.14",
    "@vue/cli-service": "~4.5.14",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.2.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-vue": "^2.0.2",
    "chromatic": "^5.10.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^7.19.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.4.1",
    "sass": "^1.43.2",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "style-resources-loader": "^1.4.1",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
